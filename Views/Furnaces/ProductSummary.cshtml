@model Furnace

<div class="card card-outline-primary m-1 p-1">
    <div class="bg-faded p-1">
        <h4>
            @Model.brand
            <span class="badge badge-pill badge-primary"
                  style="float:right">
                <small>@Model.serialnumber</small>
            </span>
        </h4>
    </div>
    <div class="cart-text p-1"> @Model.modelnumber </div>
</div>

